@model List<UserViewModel>

<div class="text-center">
    <h1 class="display-4">This is Users Page</h1>

    <div>
        <table class="table">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Contact Number</th>
                    <th>Email</th>
                    <th>User Type</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var user in Model)
                {
                    <tr>
                        <td>@user.FirstName</td>
                        <td>@user.LastName</td>
                        <td>@user.ContactNumber</td>
                        <td>@user.Email</td>
                        <td>@user.UserType</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
